@GNOME_TRUE@	for lc in C $(_DOC_REAL_LINGUAS); do \
@GNOME_TRUE@	  if test -f "$$lc"; \
@GNOME_TRUE@	    then d=; \
@GNOME_TRUE@	    xmlpath="$$lc"; \
@GNOME_TRUE@	  else \
@GNOME_TRUE@	    d="$(srcdir)/"; \
@GNOME_TRUE@	    xmlpath="$$lc:$(srcdir)/$$lc"; \
@GNOME_TRUE@	  fi; \
@GNOME_TRUE@	  for page in $(DOC_PAGES); do \
@GNOME_TRUE@	    echo "xmllint --noout --noent --path $$xmlpath --xinclude $$d$$lc/$$page"; \
@GNOME_TRUE@	    xmllint --noout --noent --path "$$xmlpath" --xinclude "$$d$$lc/$$page"; \
@GNOME_TRUE@	  done; \
@GNOME_TRUE@	done

@GNOME_TRUE@check-doc-omf: $(_DOC_OMF_ALL)
@GNOME_TRUE@	@list='$(_DOC_OMF_ALL)'; for omf in $$list; do \
@GNOME_TRUE@	  echo "xmllint --noout --xinclude --dtdvalid 'http://scrollkeeper.sourceforge.net/dtds/scrollkeeper-omf-1.0/scrollkeeper-omf.dtd' $$omf"; \
@GNOME_TRUE@	  xmllint --noout --xinclude --dtdvalid 'http://scrollkeeper.sourceforge.net/dtds/scrollkeeper-omf-1.0/scrollkeeper-omf.dtd' $$omf; \
@GNOME_TRUE@	done

